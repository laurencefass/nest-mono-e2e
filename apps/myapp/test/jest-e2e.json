{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/mylib": [
      "../../../libs/mylib/src"
    ],
    "@app/mylib/*": [
      "../../../libs/mylib/src/*"
    ],
    "@app/myconfig": [
      "../../../libs/myconfig/src"
    ],
    "@app/myconfig/*": [
      "../../../libs/myconfig/src/*"
    ],
    "@app/mypackage": [
      "../../../libs/mypackage/src"
    ],
    "@app/mypackage/*": [
      "../../../libs/mypackage/src/*"
    ]
  }
}
